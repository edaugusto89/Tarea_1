lista1=["Juan", 5, 5.7, True, 5]
print(type(lista1))
print(len(lista1))
tupla1=("Juan", 5, 5.7, True, 5)
print(type(tupla1))
print(len(tupla1))
print(lista1[0],"\n")
print(lista1[1],"\n"*2)
print(lista1[2],"\n")
print(lista1[-1],"\n")
print(lista1[-5],"\n"*3)
print(tupla1[0],"\n")
print(tupla1[1],"\n"*2)
print(tupla1[2],"\n"*3)
lista1[1]="Carlos"
print(lista1)
del lista1[2]
print(tupla1)
